<!DOCTYPE html>                                                                                                                                                                                       
<html>
  <head>
    <title>WildCard Ultimate</title>
    <%= favicon_link_tag 'favicon.ico' %>
    <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0">
    <link href='http://fonts.googleapis.com/css?family=Lato:100,900' rel='stylesheet' type='text/css'>
    <%= stylesheet_link_tag "//maxcdn.bootstrapcdn.com/bootswatch/3.3.4/yeti/bootstrap.min.css", :media => "all" %>    
    <%= stylesheet_link_tag "//netdna.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css", :media => "all" %>    
    <%= stylesheet_link_tag "home", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
  </head>
   
  <body>
      <%= yield %>
  </body>

</html>
